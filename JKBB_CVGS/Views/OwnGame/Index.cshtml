@model IEnumerable<JKBB_CVGS.Models.OwnGame>

@{
    ViewBag.Title = "Library";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Game.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Game.Developer)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Game.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Game.Developer)
        </td>
        <td>
            @Html.ActionLink("Play", "", "")
        </td>
    </tr>
}

</table>
